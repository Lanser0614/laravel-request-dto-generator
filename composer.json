{
    "name": "bellissimopizza/laravel-request-dto-generator",
    "description": "A Laravel package to automatically generate DTOs from Request files",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "dto", "request", "validation", "generator"],
    "authors": [
        {
            "name": "Bellissimo Pizza",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "BellissimoPizza\\RequestDtoGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BellissimoPizza\\RequestDtoGenerator\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BellissimoPizza\\RequestDtoGenerator\\RequestDtoGeneratorServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
